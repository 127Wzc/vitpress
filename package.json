  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "pagefind": "^1.3.0",
    "sass": "^1.76.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.5",
    "vitepress": "1.6.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
